    <parent link="link1" />
    <child link="link2" />
    <origin xyz="0 0 0.0515" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit velocity="4.8" effort="1" lower="${-pi*0.916}" upper="${pi*0.916}" />
  </joint>

  <!-- Transmission 1 -->
  <xacro:SimpleTransmission n="1" joint="joint1" />

